{
  "name": "Oak-Cherry",
  "type": "light",
  "include": "./Oak-Dark-color-theme.json",
  "tokenColors": [
    {
      "name": "Highlights",
      "scope": [
        "punctuation.definition.lifetime.rust",
        "keyword.operator.borrow.and.rust",
        "keyword.operator.dereference.rust",
        "meta.expression.receive",
        "punctuation.definition.variable.shell",
        "keyword.operator.address"
      ],
      "settings": {
        "foreground": "#c26f9b",
        "fontStyle": ""
      }
    }
  ],
  "colors": {
    "statusBar.background": "#b44882",
    "list.focusBackground": "#b44882",
    "badge.background": "#b44882",
    "activityBar.activeFocusBorder": "#b44882",
    "banner.iconForeground": "#b44882",
    "breadcrumb.background": "#b4488220",
    "editor.foldBackground": "#b4488220",
    "editorGutter.modifiedBackground": "#b44882"
  }
}
