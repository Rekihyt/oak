{
  "name": "Oak-Cherry",
  "type": "light",
  "include": "./Oak-Dark-color-theme.json",
  "tokenColors": [
    {
      "name": "Highlights",
      "scope": [
        "punctuation.definition.lifetime.rust",
        "keyword.operator.borrow.and.rust",
        "keyword.operator.dereference.rust",
        "meta.expression.receive",
        "punctuation.definition.variable.shell",
        "keyword.operator.address",
        "keyword.operator.question"
      ],
      "settings": {
        "foreground": "#c26f9b",
        "fontStyle": ""
      }
    }
  ],
  "colors": {
    "statusBar.background": "#b44882",
    "list.focusBackground": "#c26f9b",
    // "panelTitle.activeBorder": "#b44882",
    "tab.activeForeground": "#c26f9b",
    // "activityBar.activeBorder": "#b44882",
    "tab.hoverForeground": "#b44882",
    "activityBar.foreground": "#c26f9b",

    "editorError.foreground": "#cf8282",
    "errorForeground": "#cf8282"
  }
}
