        "storage.modifier.lifetime",
        "string",
        "support.constant.font-name.css",
        "support.function.variable.quoted.single.elixir",
        "text.html.derivative"
      ],
      "settings": {
        "foreground": "#FFE1A2",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "constant.language",
        "keyword.operator.comparison.scala",
        "keyword",
        "meta.declaration.adt.idris",
