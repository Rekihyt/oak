{
  "name": "Oak-Dark",
  "type": "dark",
  "include": "./Oak-color-theme.json",
  "tokenColors": [
    {
      "name": "Documentation Comments and Strings",
      "scope": [
        "comment.block.documentation",
        "comment.documentation",
        "comment.line.documentation",
        "comment.line.double-dash.documentation",
        "comment.line.number-sign.doc-comment",
