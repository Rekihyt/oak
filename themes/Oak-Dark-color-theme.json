        "punctuation.separator.clause-head-body",
        "punctuation.separator.slice",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#4e95e2",
        "fontStyle": ""
      }
    },
    {
      "name": "Types",
      "scope": [
        "class",
        "constant.language",
        "constant.other",
        "constant.type-constructor.elm",
        "constant.unit.elm",
        "entity",
        "keyword.constant.default",
        "keyword.type",
        "meta.function.python",
        "meta.return-type",
        "meta.tag.table.toml",
        "preprocessor",
        "storage.class.std.rust",
        "storage.modifier.import",
        "storage.type",
        "support.class",
        "support.constant.core.rust",
        "support.constant.unit.haskell",
        "support.type",
        "variable.type.elm"
      ],
      "settings": {
        "foreground": "#28D69C",
        "fontStyle": ""
      }
    },
    {
      "name": "Variables",
      "scope": [
        "meta.quasi-quotation.haskell",
        "meta.template.expression.nim",
        "source.groovy.embedded",
        "source.makefile",
        "source.python",
        "support.constant.nim",
        "support.variable.js",
        "variable"
      ],
      "settings": {
        "foreground": "#a58152",
        "fontStyle": ""
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.character.format.placeholder.other",
        "constant.other.placeholder",
        "constant.other",
        "entity.other.attribute-name.class.css",
        "keyword.operator.prefix.hash.haskell",
        "meta.attribute.href.hamlet",
        "meta.block",
        "punctuation.definition.interpolation",
        "punctuation.definition.placeholder",
        "punctuation.definition.template-expression",
        "source",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.other.object.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#ad9e8b",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "constant.character.format",
        "entity.name.operator.cpp",
        "entity.other.attribute-name.pseudo-element.css",
        "keyword.operator.access.dot",
        "keyword.operator.key-value",
        "keyword.operator.namespace",
        "keyword.operator.namespace",
        "keyword.other.double-colon.haskell",
        "meta.brace.round.js",
        "meta.brace",
        "meta.bracket.square.access.cpp",
        "meta.bracket",
        "meta.declaration.adt.haskell",
        "meta.declaration.adt.idris",
        "meta.declaration.exports.purescript",
        "meta.embedded.block",
        "meta.function.definition",
        "meta.import.purescript",
        "punctuation.definition.entity",
        "punctuation.separator.annotation.python",
        "punctuation.separator.dot-access",
        "punctuation.separator.dot-access.c",
        "punctuation",
        "rainbow1",
        "support.variable.magic.python",
        "text.html.markdown"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Light Punctuation",
      "scope": [
        "constant.character.escape",
        "keyword.other.unit.hexadecimal",
        "punctuation.definition.tag.html",
        "string.quoted.byte.raw"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#C0C0C0"
      }
    }
  ],
